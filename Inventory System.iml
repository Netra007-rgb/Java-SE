class Order {
    public void placeOrder(Product product, int quantity) {
        if (product.reduceStock(quantity)) {
            double total = product.getPrice() * quantity;
            System.out.println("Order placed successfully!");
            System.out.println("Quantity Ordered: " + quantity);
            System.out.println("Total Amount: ₹" + total);
        } else {
            System.out.println("Order failed: Insufficient stock.");
        }
    }
}
public class Main
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        Inventory inventory = new Inventory();
        Order order = new Order();

        while (true) {
            System.out.println("\n===== CLOTHING STORE MENU =====");
            System.out.println("1. Show Inventory");
            System.out.println("2. Order Product");
            System.out.println("3. Exit");
            System.out.print("Enter your choice: ");

            int choice = sc.nextInt();

            switch (choice) {

                case 1:
                    inventory.showInventory();
                    break;

                case 2:
                    System.out.print("Enter Product ID: ");
                    int prodId = sc.nextInt();

                    Product selectedProduct = inventory.findProduct(prodId);

                    if (selectedProduct == null) {
                        System.out.println("❌ Product not found.");
                        break;
                    }

                    System.out.print("Enter Quantity: ");
                    int quantity = sc.nextInt();

                    order.placeOrder(selectedProduct, quantity);
                    break;

                case 3:
                    System.out.println("Thank you for shopping with us!");
                    sc.close();
                    return;

                default:
                    System.out.println("❌ Invalid choice. Please try again.");
            }
        }
    }
}












